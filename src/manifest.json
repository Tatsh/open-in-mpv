{
  "author": "Andrew Udvare <andrew@udva.re>",
  "background": {
    "service_worker": "background.js"
  },
  "description": "Open an appropriate link in mpv.",
  "homepage_url": "https://github.com/Tatsh/open-in-mpv",
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png"
  },
  "manifest_version": 3,
  "name": "Open in mpv",
  "options_ui": {
    "open_in_tab": false,
    "page": "options.html"
  },
  "permissions": ["contextMenus", "nativeMessaging", "storage"],
  "version": "0.1.3",
  "web_accessible_resources": [
    {
      "matches": ["<all_urls>"],
      "resources": ["install.html", "uninstall.html"]
    }
  ]
}
