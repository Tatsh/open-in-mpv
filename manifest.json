{
  "manifest_version": 2,
  "name": "Open in mpv",
  "description": "Open an appropriate link in mpv.",
  "version": "0.0.1",
  "author": "Andrew Udvare <andrew@udva.re>",
  "homepage_url": "https://github.com/Tatsh/open-in-mpv",
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "icons": {
    "16": "icon-16.png",
    "48": "icon-48.png"
  },
  "permissions": [
    "contextMenus",
    "nativeMessaging"
  ]
}
